{
  "name": "meme-events",
  "version": "1.0.0",
  "description": "Simple zero-dependence library that exposes CSS & JS versions of popular video memes to use in client side of a web app",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "yarn run lint && yarn run cover",
    "test:prod": "cross-env BABEL_ENV=production yarn run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "yarn test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test",
    "format": "prettier --check 'src/events/**/*.{css,js}'",
    "build": "cross-env BABEL_ENV=production babel src --out-dir dist",
    "prepublish": "yarn run clean && yarn format && yarn run lint && yarn run test && yarn run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BolotskyDev/meme-events.git"
  },
  "keywords": [
    "npm",
    "module",
    "javascript",
    "browser",
    "UI",
    "fun"
  ],
  "author": "Kirill Bolotsky <frontend@bolotsky.dev> (https://bolotsky.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BolotskyDev/meme-events/issues"
  },
  "homepage": "https://github.com/BolotskyDev/meme-events#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
