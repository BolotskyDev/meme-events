{
  "name": "meme-events",
  "version": "1.0.0",
  "description": "Plug-and-play CSS/JS versions of popular video memes. Framework agnistic & dependency free",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "yarn lint && yarn format --write && yarn test:unit && yarn test:int",
    "test:unit": "jest src/.*/.*.unit.test.js --config=jest.config.js --colors",
    "test:unit:watch": "jest src/.*/.*.unit.test.js --config=jest.config.js --colors --watch",
    "test:int": "jest tests/.*/.*.int.test.js --config=jest.config.js --colors",
    "test:int:watch": "jest tests/.*/.*.int.test.js --config=jest.config.js --colors --watch",
    "coverage": "jest '(tests|src)/.*/.*.(unit|int).test.js' --config=jest.config.js --colors --collectCoverage",
    "test:fn:chrome": "testcafe chromium-browser tests/memeEvents.fn.test.js",
    "test:fn:ff": "testcafe firefox tests/memeEvents.fn.test.js",
    "lint": "eslint src tests",
    "format": "prettier --check 'src/**/*.{css,js}'",
    "build": "webpack --progress",
    "prepublish": "yarn clean && yarn format && yarn lint && yarn test && yarn build"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BolotskyDev/meme-events.git"
  },
  "keywords": [
    "npm",
    "module",
    "javascript",
    "browser",
    "UI",
    "fun"
  ],
  "author": "Kirill Bolotsky <frontend@bolotsky.dev> (https://bolotsky.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BolotskyDev/meme-events/issues"
  },
  "homepage": "https://github.com/BolotskyDev/meme-events#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "testcafe": "^1.4.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {}
}
